#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n > 0) printf("Positivo");
    else if (n < 0) printf("Negativo");
    else printf("Cero");
    return 0;
}

#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n % 2 == 0) printf("Par");
    else printf("Impar");
    return 0;
}

#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n % 5 == 0) printf("Es multiplo de 5");
    else printf("No es multiplo de 5");
    return 0;
}

#include <stdio.h>
int main() {
    int a,b,c,mayor;
    scanf("%d%d%d",&a,&b,&c);
    mayor=a;
    if(b>mayor) mayor=b;
    if(c>mayor) mayor=c;
    printf("Mayor: %d",mayor);
    return 0;
}


#include <stdio.h>
int main() {
    int a,b,c,temp;
    scanf("%d%d%d",&a,&b,&c);
    if(a<b){temp=a;a=b;b=temp;}
    if(a<c){temp=a;a=c;c=temp;}
    if(b<c){temp=b;b=c;c=temp;}
    printf("%d %d %d",a,b,c);
    return 0;
}


#include <stdio.h>
int main(){
    int a;
    scanf("%d",&a);
    if((a%4==0 && a%100!=0) || a%400==0)
        printf("Bisiesto");
    else
        printf("No bisiesto");
    return 0;
}


#include <stdio.h>
int main(){
    float a,b,c,p;
    scanf("%f%f%f",&a,&b,&c);
    p=(a+b+c)/3;
    printf("Promedio: %.2f\n",p);
    if(p>=6) printf("Aprueba");
    else printf("Reprueba");
    return 0;
}


#include <stdio.h>
int main(){
    float horas,pago,total;
    scanf("%f%f",&horas,&pago);
    if(horas<=40) total=horas*pago;
    else total=40*pago+(horas-40)*pago*2;
    printf("Salario: %.2f",total);
    return 0;
}


#include <stdio.h>
int main(){
    float min,costo;
    scanf("%f",&min);
    if(min<=3) costo=5;
    else costo=5+(min-3)*2;
    printf("Costo: %.2f",costo);
    return 0;
}


#include <stdio.h>
int main(){
    float precio;
    scanf("%f",&precio);
    if(precio>1000) precio*=0.8;
    else if(precio>500) precio*=0.9;
    printf("Total: %.2f",precio);
    return 0;
}







#include <stdio.h>
int main(){
    for(int i=1;i<=10;i++)
        printf("%d ",i);
    return 0;
}


#include <stdio.h>
int main(){
    for(int i=10;i>=1;i--)
        printf("%d ",i);
    return 0;
}


#include <stdio.h>
int main(){
    for(int i=2;i<=100;i+=2)
        printf("%d ",i);
    return 0;
}


#include <stdio.h>
int main(){
    int n;
    long suma=0;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
        suma+=i;
    printf("Suma: %ld",suma);
    return 0;
}


#include <stdio.h>
int main(){
    int n;
    long f=1;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
        f*=i;
    printf("Factorial: %ld",f);
    return 0;
}


#include <stdio.h>
int main(){
    int b,e;
    long r=1;
    scanf("%d%d",&b,&e);
    for(int i=1;i<=e;i++)
        r*=b;
    printf("Resultado: %ld",r);
    return 0;
}


#include <stdio.h>
int main(){
    int n,i,primo=1;
    scanf("%d",&n);
    if(n<=1) primo=0;
    for(i=2;i<n;i++)
        if(n%i==0) primo=0;
    if(primo) printf("Primo");
    else printf("No primo");
    return 0;
}


#include <stdio.h>
int main(){
    int n;
    long a=0,b=1,c;
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        printf("%ld ",a);
        c=a+b;
        a=b;
        b=c;
    }
    return 0;
}


#include <stdio.h>
int main(){
    int n;
    float num,suma=0;
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        scanf("%f",&num);
        suma+=num;
    }
    printf("Promedio: %.2f",suma/n);
    return 0;
}


#include <stdio.h>
int main(){
    int n,num,mayor;
    scanf("%d",&n);
    scanf("%d",&mayor);
    for(int i=2;i<=n;i++){
        scanf("%d",&num);
        if(num>mayor) mayor=num;
    }
    printf("Mayor: %d",mayor);
    return 0;
}
